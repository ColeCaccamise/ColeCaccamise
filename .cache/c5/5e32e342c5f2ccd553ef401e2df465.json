nt-size: 62.5%; // setting the root font size to be used with rem\n  scroll-behavior: smooth;\n}\n\nbody {\n  background-color: $bg-color;\n  color: $text-color;\n  box-sizing: border-box;\n  line-height: 1.7;\n  font-family: \"Poppins\", sans-serif;\n}\n\nh1,\nh2,\nh3,\nh4,\nh5,\nh6 {\n  color: $white-color;\n}\n\nul {\n  list-style: none;\n}\n\na {\n  text-decoration: none;\n  color: $link-color;\n\n  &:hover {\n    color: $hover-color;\n  }\n}\n\ni {\n  color: $text-color;\n}\n\np {\n  font-size: 1.6rem;\n  color: $text-color;\n}\n\n.container {\n  max-width: 119rem;\n  margin: 2.5rem auto;\n}\n\n.break {\n  display: block;\n}\n\n.bold {\n  font-weight: 700;\n  letter-spacing: 1.1;\n}\n\n.underline {\n  text-decoration: underline;\n}\n\nsection {\n  padding: 5rem 0;\n}\n\n// Navbar\n.navbar__container {\n  display: flex;\n  justify-content: space-between;\n  a {\n    font-size: 1.6rem;\n  }\n}\n\n.navbar__menu {\n  display: flex;\n  // justify-content: center;\n\n  li {\n    // margin-right: 12px;\n    padding: 1rem;\n  }\n\n  a {\n    font-weight: 600;\n\n    &:hover {\n      border-bottom: 1px solid black;\n    }\n  }\n}\n\n.navbar__logo {\n  cursor: pointer;\n  font-weight: 700;\n  color: $white-color;\n  text-align: center;\n  a {\n    color: $white-color;\n    font-size: 3rem;\n\n    &:hover {\n      color: $logo-hover;\n    }\n  }\n}\n\n// Hero section\nh1 {\n  font-weight: 600;\n  font-size: 6rem;\n  line-height: 7rem;\n}\n\n.hero__container {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.hero__text-wrapper {\n  display: flex;\n  flex-direction: column;\n  gap: $hero-gap;\n}\n\n.hero__copy {\n  width: 42rem;\n  line-height: 1.5;\n\n  p {\n    font-size: 1.9rem;\n    line-height: 3.5rem;\n    letter-spacing: 0.7;\n  }\n}\n\n.hero__cta-buttons {\n  margin: 10px 0;\n}\n\n@keyframes updown {\n  0% {\n    transform: translateY(0);\n  }\n\n  100% {\n    transform: translateY(1rem);\n  }\n}\n\n.chevron-container {\n  display: block;\n  text-align: center;\n  animation: updown infinite alternate 1.5s ease-in-out;\n\n  i {\n    font-size: 15px;\n  }\n}\n\n.btn {\n  padding: 1rem 1.5rem;\n  border-radius: $radius;\n  margin-right: 2rem;\n  font-weight: 500;\n  font-size: 1.6rem;\n}\n\n.btn--primary {\n  background: $white-color;\n  color: black;\n\n  &:hover {\n    color: rgba(0, 0, 0, 0.7);\n  }\n}\n\n.btn--secondary {\n  border: 1px solid $white-color;\n  color: $white-color;\n}\n\n.hero__img {\n  width: 35rem;\n}\n\n.img--round {\n  border-radius: 50%;\n}\n\n// About\n#about {\n  .about {\n    &__container {\n      text-align: center;\n    }\n\n    &__heading {\n      display: flex;\n      justify-content: space-evenly;\n      font-size: 2.8rem;\n    }\n\n    &__subheading {\n      font-size: 2rem;\n      color: gray;\n      line-height: 1;\n    }\n\n    &__text {\n      margin: 2rem 0;\n\n      p {\n        line-height: 1.6;\n      }\n    }\n  }\n}\n\n// cards\n.cards {\n  display: flex;\n\n  .card {\n    background-color: white;\n    margin: 0 1.1rem;\n    padding: 3rem;\n    text-align: left;\n    border-radius: $radius;\n    width: 33%;\n\n    h3 {\n      color: black;\n      font-size: 2.2rem;\n    }\n\n    &__description {\n      color: black;\n      margin-bottom: 2rem;\n    }\n\n    & .underline {\n      color: black;\n\n      &:hover {\n        opacity: 0.7;\n      }\n    }\n\n    &__cta {\n      font-size: 1.5rem;\n      font-weight: 500;\n\n      &:hover {\n        text-decoration: underline;\n        color: gray;\n      }\n    }\n\n    &__icons {\n      display: flex;\n      margin-bottom: 1.5rem;\n    }\n\n    &__icon {\n      font-size: 3.8rem;\n      margin-right: 1.5rem;\n\n      i {\n        color: gray;\n\n        &:hover {\n          color: black;\n        }\n      }\n    }\n  }\n}\n\n.line {\n  border: 1px solid $text-color;\n  opacity: 0.7;\n  width: 119rem;\n  border-radius: $radius;\n  margin: auto;\n}\n\n// Footer\n.footer {\n  &__container {\n    display: flex;\n    justify-content: space-around;\n    align-items: center;\n  }\n\n  .footer-name {\n    color: white;\n    font-weight: 500;\n  }\n\n  .copyright {\n    text-align: center;\n    padding: 2rem 0;\n  }\n\n  &__left {\n    width: 50%;\n\n    p {\n      margin-top: 1.5rem;\n    }\n  }\n\n  &__name {\n    display: flex;\n    align-items: center;\n\n    h4 {\n      font-size: 1.9rem;\n    }\n  }\n\n  &__img {\n    width: 7rem;\n    margin-right: 1.4rem;\n  }\n\n  &__socials {\n    margin-top: 1rem;\n  }\n\n  &__icon i {\n    font-size: 3rem;\n    margin-right: 1rem;\n  }\n\n  &__right {\n    h3 {\n      font-size: 1.8em;\n      margin-bottom: 1.2rem;\n    }\n\n    .footer__links {\n      flex-direction: column;\n\n      li {\n        font-size: 1.5rem;\n        text-align: center;\n      }\n    }\n  }\n}\n\n// Links\n.links {\n  &__container {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n  }\n}\n","scss/_variables.scss":"$bg-color: #212121;\n$white-color: #f9f9fb;\n// $text-color: #a6a5a3;\n$text-color: rgba(255, 255, 255, 0.7);\n$link-color: #888888;\n$hover-color: rgba(255, 255, 255, 0.6);\n$logo-hover: rgba(255, 255, 255, 0.85);\n$test-color: #425466;\n$hero-gap: 2rem;\n$section-gap: 2rem;\n$radius: 6px;\n"},"lineCount":null}},"error":null,"hash":"12e90976910e2376c38b5f1b75dab62c","cacheData":{"env":{}}}